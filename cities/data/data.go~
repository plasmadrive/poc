package data

type City struct {
	DisplayName string
	CityGeoId string
	CityName string
	AreaGeoId string
	AreaName string
	CuntryGeoId string
	CountryName string
	PortIndicator bool

}


func RetrieveCitiesByName(name string) []City {
	city1 := City{
		"Felixstowe,Kent,United Kingdom",
		"1",
		"Felixstowe",
		"2",
		"Kent",
		"3",
		"United Kingodom",
		true,
	}

	cityList := []City{city1}
	return cityList

}
